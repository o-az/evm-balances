    "@babel/core": "^7.17.10",
    "@swc/core": "^1.2.177",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "babel-jest": "^28.0.3",
    "dotenv": "^16.0.0",
    "eslint-config-custom": "*",
    "jest": "^27.5.0",
    "jest-environment-node": "^27.5.0",
    "pino-pretty": "^7.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig": "*",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.4"
